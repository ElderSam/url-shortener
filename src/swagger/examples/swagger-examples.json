{
  "auth": {
    "register": {
      "request": {
        "default": {
          "summary": "Valid request",
          "value": { "email": "user@email.com", "password": "password123" }
        }
      },
      "responses": {
        "success": {
          "summary": "User registered successfully",
          "value": { "id": "uuid", "email": "user@email.com" }
        },
        "validationError": {
          "summary": "Validation error",
          "value": { "statusCode": 400, "message": "Email is required", "error": "Bad Request" }
        },
        "conflictError": {
          "summary": "Email already registered",
          "value": { "statusCode": 409, "message": "Email already registered", "error": "Conflict" }
        }
      }
    },
    "login": {
      "request": {
        "default": {
          "summary": "Valid login request",
          "value": { "email": "user@email.com", "password": "password123" }
        }
      },
      "responses": {
        "success": {
          "summary": "Login successful",
          "value": { "data": { "accessToken": "jwt-token" }, "message": "Login successful" }
        },
        "validationError": {
          "summary": "Validation error",
          "value": { "statusCode": 400, "message": "Email is required", "error": "Bad Request" }
        },
        "invalidCredentials": {
          "summary": "Invalid credentials",
          "value": { "statusCode": 401, "message": "Invalid credentials", "error": "Unauthorized" }
        },
        "rateLimit": {
          "summary": "Too many login attempts",
          "value": { "statusCode": 400, "message": "Too many login attempts. Please try again later.", "error": "Bad Request" }
        }
      }
    }
  },
  "shorten": {
    "shorten": {
      "request": {
        "default": {
          "summary": "Valid request",
          "value": { "originalUrl": "https://example.com", "alias": "myalias" }
        }
      },
      "responses": {
        "success": {
          "summary": "Shortened URL created",
          "value": { "shortUrl": "https://short.ly/myalias", "originalUrl": "https://example.com", "alias": "myalias" }
        },
        "invalidInput": {
          "summary": "Invalid input",
          "value": { "statusCode": 400, "message": "URL must be valid and include http:// or https://", "error": "Bad Request" }
        }
      }
    }
  }
}
