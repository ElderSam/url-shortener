{
  "auth": {
    "register": {
      "request": {
        "default": {
          "summary": "Valid request",
          "value": { "email": "user@email.com", "password": "password123" }
        }
      },
      "responses": {
        "success": {
          "summary": "User registered successfully",
          "value": { "id": "uuid", "email": "user@email.com" }
        },
        "validationError": {
          "summary": "Validation error",
          "value": { "statusCode": 400, "message": "Email is required", "error": "Bad Request" }
        },
        "conflictError": {
          "summary": "Email already registered",
          "value": { "statusCode": 409, "message": "Email already registered", "error": "Conflict" }
        }
      }
    },
    "login": {
      "request": {
        "default": {
          "summary": "Valid login request",
          "value": { "email": "user@email.com", "password": "password123" }
        }
      },
      "responses": {
        "success": {
          "summary": "Login successful",
          "value": { "data": { "accessToken": "jwt-token" }, "message": "Login successful" }
        },
        "validationError": {
          "summary": "Validation error",
          "value": { "statusCode": 400, "message": "Email is required", "error": "Bad Request" }
        },
        "invalidCredentials": {
          "summary": "Invalid credentials",
          "value": { "statusCode": 401, "message": "Invalid credentials", "error": "Unauthorized" }
        },
        "rateLimit": {
          "summary": "Too many login attempts",
          "value": { "statusCode": 400, "message": "Too many login attempts. Please try again later.", "error": "Bad Request" }
        }
      }
    }
  },
  "shorten": {
    "shorten": {
      "request": {
        "default": {
          "summary": "Valid request (anonymous)",
          "value": { "originalUrl": "https://example.com", "alias": "myalias" }
        },
        "withBearerToken": {
          "summary": "Valid request with Bearer Token",
          "headers": { "Authorization": "Bearer <jwt-token>" },
          "value": { "originalUrl": "https://example.com", "alias": "myalias" }
        }
      },
      "responses": {
        "success": {
          "summary": "Shortened URL created",
          "value": { "shortUrl": "http://localhost:3000/myalias", "ownerId": "user-id-or-null" }
        },
        "invalidInput": {
          "summary": "Invalid input",
          "value": { "statusCode": 400, "message": "URL must be valid and include http:// or https://", "error": "Bad Request" }
        }
        ,
        "invalidToken": {
          "summary": "Invalid or expired token",
          "value": { "statusCode": 401, "message": "Invalid or expired token", "error": "Unauthorized" }
        }
      }
    },
    "redirect": {
      "parameters": {
        "default": {
          "summary": "Redirect by slug",
          "value": "abc123"
        },
        "byAlias": {
          "summary": "Redirect by alias",
          "value": "myalias"
        }
      },
      "responses": {
        "redirect": {
          "summary": "Redirect to original URL (302)",
          "headers": { "Location": "https://example.com" }
        },
        "notFound": {
          "summary": "Short URL not found",
          "value": { "statusCode": 404, "message": "Short URL not found", "error": "Not Found" }
        }
      }
    },
    "myUrls": {
      "responses": {
        "success": {
          "summary": "List of user's short URLs",
          "value": {
            "data": [
              {
                "id": "uuid-1",
                "originalUrl": "https://example.com",
                "slug": "abc123",
                "alias": null,
                "shortUrl": "http://localhost:3000/abc123",
                "accessCount": 5,
                "createdAt": "2024-01-01T00:00:00.000Z",
                "updatedAt": "2024-01-01T00:00:00.000Z"
              },
              {
                "id": "uuid-2",
                "originalUrl": "https://github.com",
                "slug": "def456",
                "alias": "myalias",
                "shortUrl": "http://localhost:3000/myalias",
                "accessCount": 10,
                "createdAt": "2024-01-02T00:00:00.000Z",
                "updatedAt": "2024-01-02T00:00:00.000Z"
              }
            ]
          }
        },
        "empty": {
          "summary": "No URLs found",
          "value": { "data": [] }
        },
        "missingToken": {
          "summary": "Missing authentication token",
          "value": { "statusCode": 401, "message": "Unauthorized", "error": "Unauthorized" }
        },
        "invalidToken": {
          "summary": "Invalid or expired token",
          "value": { "statusCode": 401, "message": "Invalid or expired token", "error": "Unauthorized" }
        }
      }
    },
    "updateUrl": {
      "request": {
        "update": {
          "summary": "Update URL request",
          "value": {
            "originalUrl": "https://updated-example.com"
          }
        }
      },
      "responses": {
        "success": {
          "summary": "Successfully updated URL",
          "value": {
            "id": "uuid-1",
            "originalUrl": "https://updated-example.com",
            "slug": "abc123",
            "alias": null,
            "shortUrl": "http://localhost:3000/abc123",
            "accessCount": 5,
            "createdAt": "2024-01-01T00:00:00.000Z",
            "updatedAt": "2024-01-10T00:00:00.000Z"
          }
        },
        "invalidUrl": {
          "summary": "Invalid URL format",
          "value": {
            "statusCode": 400,
            "message": ["URL must be valid and include http:// or https://"],
            "error": "Bad Request"
          }
        },
        "missingToken": {
          "summary": "Missing authentication token",
          "value": { "statusCode": 401, "message": "Unauthorized", "error": "Unauthorized" }
        },
        "invalidToken": {
          "summary": "Invalid or expired token",
          "value": { "statusCode": 401, "message": "Invalid or expired token", "error": "Unauthorized" }
        },
        "forbidden": {
          "summary": "User does not own this URL",
          "value": {
            "statusCode": 403,
            "message": "You do not have permission to update this URL",
            "error": "Forbidden"
          }
        },
        "notFound": {
          "summary": "URL not found or deleted",
          "value": {
            "statusCode": 404,
            "message": "Short URL not found",
            "error": "Not Found"
          }
        }
      }
    },
    "deleteUrl": {
      "responses": {
        "success": {
          "summary": "Successfully deleted (204 No Content)",
          "value": null
        },
        "missingToken": {
          "summary": "Missing authentication token",
          "value": { "statusCode": 401, "message": "Unauthorized", "error": "Unauthorized" }
        },
        "invalidToken": {
          "summary": "Invalid or expired token",
          "value": { "statusCode": 401, "message": "Invalid or expired token", "error": "Unauthorized" }
        },
        "forbidden": {
          "summary": "User does not own this URL",
          "value": {
            "statusCode": 403,
            "message": "You do not have permission to delete this URL",
            "error": "Forbidden"
          }
        },
        "notFound": {
          "summary": "URL not found or already deleted",
          "value": {
            "statusCode": 404,
            "message": "Short URL not found",
            "error": "Not Found"
          }
        }
      }
    }
  }
}
